{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "dist",
          "clean": true
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "https://docplus-backend-ruby.vercel.app/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "status": 200
      },
      {
        "src": "/assets/(.*)",
        "headers": {
          "Cache-Control": "public, max-age=31536000, immutable"
        }
      }
    ]
  }